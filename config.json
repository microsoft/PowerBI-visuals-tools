{
    "templates": {
        "visuals": "templates/visuals",
        "pbiviz": "templates/pbiviz.json.template",
        "plugin": "templates/plugin.ts.template",
        "package": "templates/package.json.template"
    },
    "generate": {
        "apiVersion": "3.8.0"
    },
    "build": {
        "precompileFolder": ".tmp/precompile",
        "dropFolder": ".tmp/drop",
        "js": "tmp.visual.js",
        "css": "visual.css",
        "stats": "../../webpack.statistics.html"
    },
    "package": {
        "dropFolder": "dist"
    },
    "server": {
        "root": "webRoot",
        "assetsRoute": "/assets",
        "privateKey": "certs/PowerBICustomVisualTest_private.key",
        "certificate": "certs/PowerBICustomVisualTest_public.crt",
        "pfx": "certs/PowerBICustomVisualTest_public.pfx",
        "port": "8080"
    },
    "visualTemplates": {
        "circlecard": "http://github.com/microsoft/powerbi-visuals-circlecard-react/archive/master.zip"
    }
}